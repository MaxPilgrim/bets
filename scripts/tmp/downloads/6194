                function cf(formObj) {
                    for (var i = 0; i < formObj.elements.length; i++) {
                        if ((formObj.elements[i].type == 'text') || (formObj.elements[i].type == 'textarea')) {
                            formObj.elements[i].value = formObj.elements[i].value.trim();
                        }
                    }
                    return false;
                }
            </script>
