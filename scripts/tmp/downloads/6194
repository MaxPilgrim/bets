<script language="javascript">
function hideS( newSport )
{
  trO = document.getElementById( "trs" + newSport );
  imO = document.getElementById( "ims" + newSport );
  lgO = document.getElementById( "lg" + newSport );
  jQuery("#trs" + newSport).removeClass("viewSport");
  //if ( trO ) { trO.style.background='#F6F6F6'; }
  if ( imO ) { imO.src='http://ru.leonbets.com/img/bets102/sport_li.jpg'; }
  if ( lgO ) { lgO.style.display='none'; }
}

function showS( newSport )
{
  lgO = document.getElementById( "lg" + newSport );
  if ( lgO && lgO.style.display == 'block' )
  {
    hideS( newSport );
    return;
  }
  imO = document.getElementById( "ims" + newSport );
  trO = document.getElementById( "trs" + newSport );
  //if ( trO ) { trO.style.background='#ffffff'; }
  jQuery("#trs" + newSport).addClass("viewSport");
  if ( imO ) { imO.src='http://ru.leonbets.com/img/bets102/on_sport_li.jpg'; }
  if ( lgO ) { lgO.style.display='block'; }
}
function overS( newSport )
{
  lgO = document.getElementById( "lg" + newSport );
  if ( lgO && lgO.style.display == 'block' )
  {
    return;
  }

if ( newSport == 1 )
{
  return;
}

  document.getElementById('trs' + newSport ).style.background='#ffffff';
}
function outS( newSport )
{
  lgO = document.getElementById( "lg" + newSport );
  if ( lgO && lgO.style.display == 'block' )
  {
    return;
  }

if ( newSport == 1 )
{
  return;
}

  document.getElementById('trs' + newSport ).style.background='#F6F6F6';
}
</script>


